programa
{
	
	funcao inicio()
	{
		inteiro a = 100, b
		b = incrementa(a)
		escreva("O valor do incremento é: ", incrementa(a),"\n")
		escreva("O valor da variável a é: ",a,"\n")
		escreva("O valor da variável b é: ",b)
	}
	funcao inteiro incrementa(inteiro i){
		retorne i + 10
	}
}
